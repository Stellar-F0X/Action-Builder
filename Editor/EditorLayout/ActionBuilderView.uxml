            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="right-view" class="splitted-view" style="margin-left: 1px; margin-right: 1px; flex-shrink: 1; flex-basis: auto; left: auto; min-width: 220px;">
            <ui:Label text="Inspector" name="title" class="title" />
