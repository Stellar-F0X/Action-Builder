                <ui:Button name="create-action-button" style="background-image: url(&quot;project://database/Assets/Action-Builder/Editor/EditorLayout/Add.png?fileID=2800000&amp;guid=80311de3d13fbfc48983f393c313696d&amp;type=3#Add&quot;); flex-grow: 0; flex-shrink: 1; height: 25px; width: 25px; min-width: initial; min-height: initial; padding-right: 1px; padding-left: 1px; margin-top: 3px; margin-right: 1px; margin-bottom: 3px; margin-left: 1px;" />
                <ui:Button name="delete-action-button" style="background-image: url(&quot;project://database/Assets/Action-Builder/Editor/EditorLayout/Remove.png?fileID=2800000&amp;guid=43dbf9a639d9a884aa38a785667ec1e9&amp;type=3#Remove&quot;); flex-grow: 0; flex-shrink: 1; height: 25px; width: 25px; min-width: initial; min-height: initial; padding-right: 1px; padding-left: 1px; margin-top: 3px; margin-right: 1px; margin-bottom: 3px; margin-left: 1px;" />
            </ui:VisualElement>
            <ui:VisualElement name="action-container" style="flex-grow: 1;">
