.line {
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-left-color: rgb(40, 40, 40);
    border-right-color: rgb(40, 40, 40);
    border-top-color: rgb(40, 40, 40);
    border-bottom-color: rgb(40, 40, 40);
    margin-left: 0;
    margin-right: 0;
    margin-top: 3px;
    margin-bottom: 3px;
    width: 100%;
    height: 0;
}

.label-header {
    font-size: 13px;
    -unity-font-style: bold;
    -unity-text-align: middle-left;
    overflow: hidden;
    visibility: visible;
    display: flex;
    flex-grow: 1;
    margin-top: 3px;
    margin-bottom: 2px;
    padding-left: 2px;
    padding-right: 2px;
    border-bottom-width: 2px;
    border-left-color: rgb(45, 45, 45);
    border-right-color: rgb(45, 45, 45);
    border-top-color: rgb(45, 45, 45);
    border-bottom-color: rgb(45, 45, 45);
    background-color: rgba(255, 255, 255, 0.04);
    border-bottom-left-radius: 1px;
    border-bottom-right-radius: 1px;
    padding-bottom: 2px;
    margin-left: 0;
    margin-right: 0;
    width: 100%;
}

#description-content > #unity-text-input {
    height: 50px;
    min-height: initial;
    max-height: initial;
}

#container {
    flex-grow: 0;
}

#container > #main-header > #unity-content {
    margin-left: 5px;
}

Label {
    width: 150px;
}

#container > .unity-foldout--depth-0 > .unity-foldout__toggle {
    flex-grow: 1;
    width: 100%;
    padding-left: 2px;
    padding-bottom: 2px;
    padding-right: 2px;
    margin-bottom: 0;
    margin-top: 3px;
    font-size: 13px;
    -unity-text-align: middle-left;
    background-color: rgba(255, 255, 255, 0.04);
    border-left-color: rgb(45, 45, 45);
    border-right-color: rgb(45, 45, 45);
    border-top-color: rgb(45, 45, 45);
    border-bottom-color: rgb(45, 45, 45);
    border-bottom-width: 1px;
    border-bottom-left-radius: 1px;
    border-bottom-right-radius: 1px;
    margin-left: 0;
}

#container > #main-header > .unity-foldout__toggle > .unity-foldout__input {
    margin-left: 21px;
}
